    <groupId>com.dummy.project</groupId>
    <artifactId>letsLearn</artifactId>
    <version>1.0-SNAPSHOT</version>

    <dependencies>
        <!--This dependency is used to identify keywords like @Test etc -->
        <dependency>
            <groupId>org.testng</groupId>
            <artifactId>testng</artifactId>
            <version>6.8</version>
        </dependency>
        <!--This dependency is used to understand selenium commands like driver.close etc -->
