        <!--Related files: baseClass>line#46>setEnv(), lib>envConfig.java  -->
        <dependency>
            <groupId>javax.xml.bind</groupId>
            <artifactId>jaxb-api</artifactId>
            <version>2.2.6</version>
        </dependency>
    </dependencies>

</project>